{
  "name": "strapi-xlsx-to-json-translate",
  "version": "2.0.1",
  "description": "This plugin allows you to upload an XLSX file containing translations and outputs them as JSON data.",
  "keywords": [
    "xlsx",
    "json",
    "translate",
    "i18n",
    "internationalization",
    "strapi",
    "convert"
  ],
  "author": "Etienne PASTEUR <me@etiennepasteur.com>",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-xlsx-to-json-translate",
    "displayName": "Strapi XLSX to JSON Translate",
    "description": "This plugin allows you to upload an XLSX file containing translations and outputs them as JSON data."
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react-intl": "^7.1.14",
    "xlsx": "file:vendor/xlsx-0.20.3.tgz"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5",
    "@strapi/strapi": "^5",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6",
    "styled-components": "^6"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5",
    "@strapi/strapi": "^5",
    "@strapi/typescript-utils": "^5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "prettier": "^3.6.2",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6",
    "styled-components": "^6",
    "typescript": "^5"
  },
  "bundledDependencies": [
    "xlsx"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
